      <ProgramUnit Name="BUSCAR" ProgramUnitType="Procedimiento" ProgramUnitText="PROCEDURE buscar IS&amp;#10;BEGIN&amp;#10;   go_block('req_categoria');&amp;#10;   Execute_Query(All_Records);&amp;#10;END;"/>
      <ProgramUnit Name="IMPRIMIR" ProgramUnitType="Procedimiento" ProgramUnitText="PROCEDURE IMPRIMIR IS&amp;#10;BEGIN&amp;#10;  NULL;&amp;#10;END;"/>
      <ProgramUnit Name="VALIDA_RANGO" ProgramUnitType="Procedimiento" ProgramUnitText="PROCEDURE valida_rango (valor in number,&amp;#10;                        valido in out varchar2) IS&amp;#10;valorini number;&amp;#10;valorfin number;&amp;#10;BEGIN&amp;#10;/* Valida que el rango suministrado esté dentro de la categoria, Menor = M   Mayor = Y */&amp;#10;	begin&amp;#10;    select valor_ppto_ini, valor_ppto_fin&amp;#10;      into valorini, valorfin&amp;#10;      from categoria_ppto&amp;#10;     where c_emp = :global.empresa&amp;#10;       and ano   = to_char(sysdate,'YYYY')&amp;#10;       and cod_cat = :req_categoria.cod_cat;&amp;#10;	  exception when no_data_found then&amp;#10;		 begin&amp;#10;			valorini := 0;&amp;#10;			valorfin := 0;&amp;#10;		 end;	&amp;#10;  end;&amp;#10;  Valido := 'N';&amp;#10;  If valor between valorini and valorfin then&amp;#10;  	valido := 'S';&amp;#10;  end if;  &amp;#10;END;"/>
